{
  "name": "my-jest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woriwori/my-jest.git"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest", 
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": ["<rootDir>/node_modules/"],
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "json"
    ],
    "globals": {
        "ts-jest": {
            "diagnostics": true
        }
    }
  },
